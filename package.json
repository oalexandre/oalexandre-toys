{
  "name": "oalexandre-toys",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.20.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "staging": "cross-env STAGING='true' next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "code-quality": "npm run lint:strict && npm run format:check && npm run type-check",
    "fix-all": "npm run lint:fix && npm run format",
    "prod": "vercel --prod",
    "deploy": "vercel"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.1",
    "@emailjs/browser": "^3.10.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.7",
    "@sendgrid/mail": "^7.7.0",
    "@userback/react": "^0.3.0",
    "add": "^2.0.6",
    "axios": "^1.3.3",
    "change-case": "^4.1.2",
    "colord": "^2.9.2",
    "convert": "^4.5.0",
    "cpf-cnpj-validator": "^1.0.3",
    "dotenv": "^16.0.3",
    "easyqrcodejs": "^4.4.12",
    "file-saver": "^2.0.5",
    "formik": "^2.2.9",
    "framer-motion": "^9.1.2",
    "json-server": "^0.17.2",
    "materialize-css": "^1.0.0-rc.2",
    "next": "12.2.2",
    "next-pwa": "^5.5.4",
    "next-sitemap": "^3.1.10",
    "nodemailer": "^6.9.1",
    "phosphor-react": "^1.4.1",
    "qrcode.react": "^3.1.0",
    "react": "18.2.0",
    "react-colorful": "^5.5.1",
    "react-confetti": "^6.1.0",
    "react-dom": "18.2.0",
    "react-feedback-widget": "^4.0.0",
    "react-input-mask": "^2.0.4",
    "react-text-mask": "^5.5.0",
    "redux-form-input-masks": "^2.0.2",
    "sass": "^1.58.0",
    "typescript": "^4.9.5",
    "vercel": "^28.15.4",
    "yarn": "^1.22.22",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cross-env": "^7.0.3",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^4.2.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^2.8.8"
  }
}
